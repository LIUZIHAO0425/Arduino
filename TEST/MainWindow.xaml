        <Button x:Name="TEST7" Content="7" HorizontalAlignment="Left" Margin="158,123,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST7_Click"/>
        <Button x:Name="TEST8" Content="8" HorizontalAlignment="Left" Margin="220,123,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST8_Click"/>
