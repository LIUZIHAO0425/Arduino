        <Button x:Name="Disconnect" Content="Disconnect" HorizontalAlignment="Left" Margin="483,350,0,0" VerticalAlignment="Top" Width="71" Height="24" Click="Disconnect_Click"/>
        <Button x:Name="back" Content="back" HorizontalAlignment="Left" Margin="552,123,0,0" VerticalAlignment="Top" Width="72" Height="34" Click="back_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="465,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="31"><Run Language="zh-cn" Text="Image"/><LineBreak/><Run Language="zh-cn"/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="33,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Text="4"/><Run Text="*"/><Run Language="zh-cn" Text="4"/><Run Text=" Test"/></TextBlock>
        <Button x:Name="TEST1" Content="1" HorizontalAlignment="Left" Margin="33,65,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST1_Click"/>
        <Button x:Name="TEST2" Content="2" HorizontalAlignment="Left" Margin="95,65,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST2_Click"/>
        <Button x:Name="TEST3" Content="3" HorizontalAlignment="Left" Margin="158,65,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST3_Click"/>
        <Button x:Name="TEST4" Content="4" HorizontalAlignment="Left" Margin="220,65,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST4_Click"/>
        <Button x:Name="TEST5" Content="5" HorizontalAlignment="Left" Margin="33,123,0,0" VerticalAlignment="Top" Width="40" Height="38" Click="TEST5_Click"/>
