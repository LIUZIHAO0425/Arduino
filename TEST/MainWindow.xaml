<Window x:Class="controller_Arduino.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TEST"
        mc:Ignorable="d"
        Title="Controller For Blindpad" Height="432" Width="703">
    <Grid Margin="0,0,0,-15">
        <Button x:Name="straight" Content="straight" HorizontalAlignment="Left" Margin="465,123,0,0" VerticalAlignment="Top" Width="72" Height="34" Click="straight_Click"/>
        <Button x:Name="left" Content="left" HorizontalAlignment="Left" Margin="552,65,0,0" VerticalAlignment="Top" Width="72" Height="38" Click="lift_Click"/>
        <Button x:Name="right" Content="right" HorizontalAlignment="Left" Margin="465,65,0,0" VerticalAlignment="Top" Width="72" Height="38" Click="right_Click"/>
        <TextBlock x:Name="status" HorizontalAlignment="Left" Margin="574,354,0,0" TextWrapping="Wrap" Text="Disconnected" VerticalAlignment="Top" Height="29" Width="94"/>
        <TextBox x:Name="comportno" HorizontalAlignment="Left" Height="23" Margin="561,310,0,0" TextWrapping="Wrap" Text="COM4" VerticalAlignment="Top" Width="120"/>
